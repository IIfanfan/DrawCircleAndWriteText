<UserControl x:Class="RP.AGVDispatch.Monitor.DrawAgvCarAndWriteText"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RP.AGVDispatch.Monitor"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Width="50" Height="50">
        <Rectangle  Width="50"  Height="50" Fill="Yellow" Stroke="Blue" Name="TextRectangle"   VerticalAlignment="Center"  HorizontalAlignment="Center" StrokeThickness="3"  Loaded="Rectangle_Loaded" Grid.Column="1"></Rectangle>
        <TextBlock Text="15" Foreground="White" Name="TextName" TextAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="1"/>
    </Grid>
</UserControl>
